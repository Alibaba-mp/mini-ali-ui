<list>
  <block a:for="{{components}}">
    <navigator url="{{item.path}}" key="items-{{item.name}}"  >
      <list-item arrow="{{true}}" 
        multipleLine="{{true}}" 
        last="{{index===components.length - 1}}">
        {{item.name}}
        <view class="am-list-brief">{{item.nameZN}}</view>
      </list-item>
    </block>
  </navigator>

</list>
