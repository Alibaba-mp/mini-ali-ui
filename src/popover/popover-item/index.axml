<view class="am-popover-item {{className}}" onTap="onItemClick">
  <view class="am-popover-item-icon-holder am-popover-item-ver-center" a:if="{{iconType}}">
    <am-icon  type="{{iconType}}" size="22"/>
  </view>
  <view class="am-popover-item-slot-holder am-popover-item-ver-center" >
    <slot/>
  </view>
</view>
